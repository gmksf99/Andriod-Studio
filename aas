[32m+[m[32m@SuppressWarnings("deprecation")[m
[32m+[m[32mpublic class MainActivity extends AppCompatActivity implements ActionBar.TabListener {[m
[32m+[m[32m    MyTabFragment myFrags[] = new MyTabFragment[3];[m
[32m+[m[32m    ActionBar.Tab ca, tp, pb;[m
[32m+[m[32m    @Override[m
[32m+[m[32m    protected void onCreate(Bundle savedInstanceState) {[m
[32m+[m[32m        super.onCreate(savedInstanceState);[m
[32m+[m[32m        setContentView(R.layout.activity_main);[m
[32m+[m[32m        ActionBar bar = getSupportActionBar();[m
[32m+[m[32m        bar.setNavigationMode(ActionBar.NAVIGATION_MODE_TABS);[m
[32m+[m[32m        setTitle("과제 #3");[m
[32m+[m
[32m+[m[32m        ca = bar.newTab();[m
[32m+[m[32m        ca.setText("캘린더");[m
[32m+[m[32m        ca.setTabListener(this);[m
[32m+[m[32m        bar.addTab(ca);[m
[32m+[m
[32m+[m[32m        tp = bar.newTab();[m
[32m+[m[32m        tp.setText("타임피커");[m
[32m+[m[32m        tp.setTabListener(this);[m
[32m+[m[32m        bar.addTab(tp);[m
[32m+[m
[32m+[m[32m        pb = bar.newTab();[m
[32m+[m[32m        pb.setText("프로그래스바");[m
[32m+[m[32m        pb.setTabListener(this);[m
[32m+[m[32m        bar.addTab(pb);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public void onTabSelected(ActionBar.Tab tab, FragmentTransaction ft) {[m
[32m+[m[32m        MyTabFragment myTabFragment = null;[m
[32m+[m[32m        if(myFrags[tab.getPosition()]==null){[m
[32m+[m[32m            myTabFragment = new MyTabFragment();    //세로 생성[m
[32m+[m[32m            Bundle data = new Bundle(); //번들을 통해 데이터 전송[m
[32m+[m[32m            data.putString("tabName", tab.getText().toString());[m
[32m+[m[32m            myTabFragment.setArguments(data);   //프래그먼트로 데이터 전송[m
[32m+[m[32m            myFrags[tab.getPosition()] = myTabFragment;[m
[32m+[m[32m        }[m
[32m+[m[32m        else{[m
[32m+[m[32m            myTabFragment = myFrags[tab.getPosition()];[m
[32m+[m[32m        }[m
[32m+[m[32m        ft.replace(android.R.id.content, myTabFragment);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public void onTabUnselected(ActionBar.Tab tab, FragmentTransaction ft) {[m
[32m+[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public void onTabReselected(ActionBar.Tab tab, FragmentTransaction ft) {[m
[32m+[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public static class MyTabFragment extends androidx.fragment.app.Fragment{[m
[32m+[m[32m        String tabName;[m
[32m+[m[32m        int now;[m
[32m+[m[32m        @Override[m
[32m+[m[32m        public void onCreate(@Nullable Bundle savedInstanceState) {[m
[32m+[m[32m            super.onCreate(savedInstanceState);[m
[32m+[m[32m            Bundle data = getArguments();[m
[32m+[m[32m            tabName = data.getString("tabName");    //해당 데이터 꺼내옴[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        @Nullable[m
[32m+[m[32m        @Override[m
[32m+[m[32m        public View onCreateView(@NonNull LayoutInflater inflater, @Nullable ViewGroup container, @Nullable Bundle savedInstanceState) {    //위젯을 리턴한다.[m
[32m+[m[32m            LinearLayout.LayoutParams params = new LinearLayout.LayoutParams(LinearLayout.LayoutParams.MATCH_PARENT, LinearLayout.LayoutParams.MATCH_PARENT);   //XML을 사용하지 않아서 파라미터를 만들어줌[m
[32m+[m[32m            params.gravity = Gravity.CENTER;    //파라미터들 가운데로 정렬[m
[32m+[m[32m            LinearLayout base = new LinearLayout(super.getActivity());[m
[32m+[m[32m            base.setOrientation(LinearLayout.VERTICAL);[m
[32m+[m[32m            base.setLayoutParams(params);   //파라미터를 파람즈로[m
[32m+[m[32m            CalendarView cv = new CalendarView(getContext());[m
[32m+[m[32m            TimePicker tpi = new TimePicker(getContext());[m
[32m+[m[32m            final TextView tv1 = new TextView(getContext());    //날짜, 시간이 뜨는 텍스트[m
[32m+[m[32m            tv1.setTextSize(20);[m
[32m+[m[32m            tv1.setPadding(20,0,0,0);[m
[32m+[m[32m            final ProgressBar pgb = new ProgressBar(getContext(), null, android.R.attr.progressBarStyleHorizontal);[m
[32m+[m[32m            Button btn1 = new Button(getContext());[m
[32m+[m[32m            Button btn2 = new Button(getContext());[m
[32m+[m[32m            pgb.setPadding(10,20,10,10);[m
[32m+[m[32m            pgb.setMax(100);[m
[32m+[m[32m            pgb.setProgress(50);[m
[32m+[m[32m            if (tabName=="캘린더") {[m
[32m+[m[32m                cv.setOnDateChangeListener(new CalendarView.OnDateChangeListener() {[m
[32m+[m[32m                    @Override[m
[32m+[m[32m                    public void onSelectedDayChange(@NonNull CalendarView calendarView, int i, int i1, int i2) {[m
[32m+[m[32m                        int m = i1+1;[m
[32m+[m[32m                        tv1.setText("날짜 : "+i+"년"+m+"월"+i2+"일");[m
[32m+[m[32m                    }[m
[32m+[m[32m                });[m
[32m+[m[32m                base.addView(cv);[m
[32m+[m[32m                base.addView(tv1);[m
[32m+[m[32m            }[m
[32m+[m[32m            if (tabName=="타임피커") {[m
[32m+[m[32m                tpi.setOnTimeChangedListener(new TimePicker.OnTimeChangedListener() {[m
[32m+[m[32m                    @Override[m
[32m+[m[32m                    public void onTimeChanged(TimePicker timePicker, int i, int i1) {[m
[32m+[m[32m                        tv1.setText("시간 : "+i+"시"+i1+"분");[m
[32m+[m[32m                    }[m
[32m+[m[32m                });[m
[32m+[m[32m                base.addView(tpi);[m
[32m+[m[32m                base.addView(tv1);[m
[32m+[m[32m            }[m
[32m+[m[32m            if (tabName=="프로그래스바") {[m
[32m+[m[32m                LinearLayout base2 = new LinearLayout(super.getActivity());[m
[32m+[m[32m                base2.setLayoutParams(params);[m
[32m+[m[32m                base2.setOrientation(LinearLayout.HORIZONTAL);  //Horizontal타입 레이아웃[m
[32m+[m[32m                btn1.setText("감소");[m
[32m+[m[32m                btn2.setText("증가");[m
[32m+[m[32m                base2.setPadding(20,0,0,0);[m
[32m+[m
[32m+[m[32m                btn1.setOnClickListener(new View.OnClickListener() {[m
[32m+[m[32m                    @Override[m
[32m+[m[32m                    public void onClick(View view) {[m
[32m+[m[32m                        now = pgb.getProgress();[m
[32m+[m[32m                        now -= 5;[m
[32m+[m[32m                        pgb.setProgress(now);[m
[32m+[m[32m                    }[m
[32m+[m[32m                });[m
[32m+[m[32m                btn2.setOnClickListener(new View.OnClickListener() {[m
[32m+[m[32m                    @Override[m
[32m+[m[32m                    public void onClick(View view) {[m
[32m+[m[32m                        now = pgb.getProgress();[m
[32m+[m[32m                        now += 5;[m
[32m+[m[32m                        pgb.setProgress(now);[m
[32m+[m[32m                    }[m
[32m+[m[32m                });[m
[32m+[m[32m                base.addView(pgb);[m
[32m+[m[32m                base2.addView(btn1);[m
[32m+[m[32m                base2.addView(btn2);[m
[32m+[m[32m                base.addView(base2);    //base2에 저장한 화면을 base에 저장[m
[32m+[m[32m            }[m
