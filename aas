[32m+[m[32m        btnCancel = (Button) findViewById(R.id.button16);[m
[32m+[m[32m        btnEqual = (Button) findViewById(R.id.button17);[m
[32m+[m[32m        tv = (TextView) findViewById(R.id.textView);[m
[32m+[m
[32m+[m[32m        btnAdd.setOnTouchListener(new View.OnTouchListener() {[m
[32m+[m[32m            @Override[m
[32m+[m[32m            public boolean onTouch(View view, MotionEvent motionEvent) {[m
[32m+[m[32m                if (n1 != null) {[m
[32m+[m[32m                    if (n2 != null) {[m
[32m+[m[32m                        n1 = et1.getText().toString();[m
[32m+[m[32m                        n2 = et2.getText().toString();[m
[32m+[m[32m                        result = Double.parseDouble(n1) + Double.parseDouble(n2);[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[32m                else {      //숫자를 입력안하고 연산자를 눌렸을때[m
[32m+[m[32m                    Toast.makeText(getApplicationContext(), "숫자를 선택해주세요", Toast.LENGTH_SHORT).show();[m
[32m+[m[32m                }[m
[32m+[m[32m                return false;[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[32m        btnSub.setOnTouchListener(new View.OnTouchListener() {[m
[32m+[m[32m            @Override[m
[32m+[m[32m            public boolean onTouch(View view, MotionEvent motionEvent) {[m
[32m+[m[32m                if (n1 != null) {[m
[32m+[m[32m                    if (n2 != null) {[m
[32m+[m[32m                        n1 = et1.getText().toString();[m
[32m+[m[32m                        n2 = et2.getText().toString();[m
[32m+[m[32m                        result = Double.parseDouble(n1) - Double.parseDouble(n2);[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[32m                return false;[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[32m        btnMul.setOnTouchListener(new View.OnTouchListener() {[m
