[32m+[m[32m        et1 = (EditText) findViewById(R.id.editText);[m
[32m+[m[32m        et2 = (EditText) findViewById(R.id.editText2);[m
[32m+[m[32m        btnAdd = (Button) findViewById(R.id.button10);[m
[32m+[m[32m        btnSub = (Button) findViewById(R.id.button11);[m
[32m+[m[32m        btnMul = (Button) findViewById(R.id.button12);[m
