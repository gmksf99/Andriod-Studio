[32m+[m[32m        btnEqual.setOnClickListener(new View.OnClickListener() {[m
[32m+[m[32m            @Override[m
[32m+[m[32m            public void onClick(View view) {[m
[32m+[m[32m                if(result != null) {            // 계산된 결과가 있으면 출력 = 연산자 입력했으면 출력[m
[32m+[m[32m                    if (n2.trim().equals("0")) {[m
[32m+[m[32m                        tv.setText("결과 : 0으로 나눌 수 없습니다.");[m
[32m+[m[32m                    }[m
[32m+[m[32m                    else {[m
[32m+[m[32m                        tv.setText("결과 : " + result.toString());[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[32m                else {                      // 숫자는 입력했으나 연산자를 입력 안했다면[m
[32m+[m[32m                    Toast.makeText(getApplicationContext(), "연산자를 선택해주세요", Toast.LENGTH_SHORT).show();[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[32m        btnCancel.setOnClickListener(new View.OnClickListener() {[m
[32m+[m[32m            @Override[m
[32m+[m[32m            public void onClick(View view) {[m
[32m+[m[32m                et1.getText().clear();[m
[32m+[m[32m                et2.getText().clear();[m
[32m+[m[32m                n1 = null;[m
[32m+[m[32m                n2 = null;[m
[32m+[m[32m                result = null; //결과 초기화[m
[32m+[m[32m                tv.setText("결과 : ");[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        for (i =0; i < numBtnIDs.length; i++){[m
[32m+[m[32m            numButtons[i] = (Button) findViewById(numBtnIDs[i]);[m
[32m+[m[32m        }[m
[32m+[m[32m        for (i=0; i < numBtnIDs.length; i++) {[m
[32m+[m[32m            final int index;    // 꼭 필요[m
[32m+[m[32m            index = i;[m
[32m+[m
[32m+[m[32m            numButtons[index].setOnClickListener(new View.OnClickListener() {[m
[32m+[m[32m                public void onClick(View v) {[m
[32m+[m[32m                    if (et1.isFocused() == true) {[m
[32m+[m[32m                        n1 = et1.getText().toString() + numButtons[index].getText().toString();[m
[32m+[m[32m                        et1.setText(n1);[m
[32m+[m[32m                    } else if (et2.isFocused() == true) {[m
[32m+[m[32m                        n2 = et2.getText().toString() + numButtons[index].getText().toString();[m
[32m+[m[32m                        et2.setText(n2);[m
[32m+[m[32m                    } else {[m
[32m+[m[32m                        Toast.makeText(getApplicationContext(), "먼저 에디트텍스트를 선택하세요", Toast.LENGTH_SHORT).show();[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[32m            });[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
